[Appearance]
ColorScheme=Ubuntu Linux
Font=Monospace,12,-1,2,50,0,0,0,0,0

[General]
Command=/bin/bash --login
LocalTabTitleFormat=[%#]: (%n) - %d
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=[%#]: (%u) - %h

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=false

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
BlinkingTextEnabled=true
